;; shadow-cljs configuration
{:source-paths ["src"]
 :dev-http {8020 "public"}
 :builds
 {:app
  {:target           :browser
   :output-dir       "public/js"
   :modules          {:main {:init-fn ftree.core/main}}
   :compiler-options {:output-feature-set :es-next
                      :language-out       :ecmascript-next
                      :infer-externs      true
                      :pretty-print       false
                      :pseudo-names       false}}}}
